{
  "name": "dataviz-project-starter-kit",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.6",
    "@vitest/ui": "^0.34.7",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "dependencies": {
    "@sveltejs/svelte-scroller": "^2.0.7",
    "@urbaninstitute/dataviz-components": "^0.3.1",
    "d3": "^7.8.5",
    "layercake": "^8.0.2"
  }
}
